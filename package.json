{
    "name": "ui5-task-ts-migration",
    "version": "0.1.9",
    "description": "Migration task for ui5 projects which allows migrating UI5 AMD Sources to Typescript",
    "main": "dist/index.js",
    "bin": {
        "ui5-task-ts-migration": "dist/index.js"
    },
    "scripts": {
        "test": "npm run build && npm run jest",
        "jest": "jest",
        "coverage": "npm run jest --collect-coverage",
        "coverage-verbose": "npm run coverage --verbose",
        "build": "tsc -p ."
    },
    "files": [
        "dist/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/eldefay/ui5-task-ts-migration.git"
    },
    "keywords": [
        "migration",
        "typescript",
        "ui5",
        "tools",
        "business",
        "application",
        "studio"
    ],
    "author": "Samer Aldefai",
    "license": "Apache-2.0",
    "dependencies": {
        "@ui5/fs": "^2.0.6",
        "@ui5/logger": "^2.0.1",
        "@ui5/project": "^2.3.1",
        "glob": "^7.1.7",
        "js-yaml": "^4.1.0",
        "jsonata": "^1.7.0",
        "ts-morph": "^11.0.3",
        "typescript": "^4.3.5"
    },
    "devDependencies": {
        "@types/glob": "^7.1.4",
        "@types/jest": "^27.0.1",
        "@types/sinon": "^10.0.0",
        "jest": "^27.1.0",
        "sinon": "^11.1.0",
        "source-map-support": "^0.5.19",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.0.0"
    },
    "types": "dist/index.d.ts"
}
